
{%- if entity.has_attributes == true -%}
	{%- include 'inc/whitespace_newline' -%}
	{%- include 'inc/whitespace_newline' -%}
	{%- include 'inc/whitespace_tab' -%}
	{%- include 'inc/access_control_entity' -%}enum Attributes: String {
		{%- for attribute in entity.attributes -%}
			{%- include 'inc/whitespace_newline' -%}
			{%- if attribute.realm_ignored or attribute.realm_read_only.size == 0 -%}
				{%- if attribute.comment.size > 0 -%}
					/** {{ attribute.comment }} **/
				{%- endif -%}
				{%- include 'inc/whitespace_tab' -%}
				{%- include 'inc/whitespace_tab' -%}
				case {{ attribute.name }} = "{{ attribute.name | escape_quotes }}"
			{%- endif -%}
		{%- endfor -%}
		{%- include 'inc/whitespace_newline' -%}
		{%- include 'inc/whitespace_tab' -%}
	}
	{%- include 'inc/whitespace_newline' -%}
{%- endif -%}